{
  "name": "moltbot-feishu",
  "version": "0.1.0",
  "type": "module",
  "description": "Feishu (飞书/Lark) channel plugin for Moltbot — WebSocket long-connection, no public server required",
  "author": "Alex Yang <anyangsen@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/AlexAnys/moltbot-feishu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexAnys/moltbot-feishu.git"
  },
  "bugs": {
    "url": "https://github.com/AlexAnys/moltbot-feishu/issues"
  },
  "keywords": [
    "moltbot",
    "clawdbot",
    "feishu",
    "lark",
    "飞书",
    "chatbot",
    "ai-agent",
    "messaging",
    "channel-plugin",
    "websocket"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "clawdbot.plugin.json",
    "README.md",
    "LICENSE"
  ],
  "clawdbot": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "feishu",
      "label": "Feishu",
      "selectionLabel": "Feishu (飞书)",
      "docsPath": "/channels/feishu",
      "docsLabel": "feishu",
      "blurb": "Feishu (Lark) messaging platform with Bot WebSocket API.",
      "aliases": [
        "lark",
        "fs"
      ],
      "order": 85,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "moltbot-feishu",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "clawdbot": ">=2026.1.20"
  },
  "peerDependenciesMeta": {
    "clawdbot": {
      "optional": false
    }
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.56.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.8.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
